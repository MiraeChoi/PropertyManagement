<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		 <typeAlias type="vo.GonginVO" alias="gongin"/>
		 <typeAlias type="vo.ImdaeinVO" alias="imdaein"/>
		 <typeAlias type="vo.KwanriVO" alias="kwanri"/>
		 <typeAlias type="vo.InfoVO" alias="info"/>
		 <typeAlias type="vo.GunmulVO" alias="gunmul"/>
		 <typeAlias type="vo.PolygonVO" alias="polygon"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/budong.xml" />
		<mapper resource="config/mybatis/mapper/manage.xml" />
		<mapper resource="config/mybatis/mapper/gongin.xml" />
		<mapper resource="config/mybatis/mapper/imdaein.xml" />
	</mappers>
</configuration>










